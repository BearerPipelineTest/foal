{
  "port": 3001,
  "settings": {
    "jwt": {
      "secret": "env(SETTINGS_JWT_SECRET)",
      "csrf": {
        "enabled": true
      }
    },
    "csrf": {
      "cookie": {
        "maxAge": "31536000"
      }
    },
    "debug": false,
    "loggerFormat": "tiny",
    "session": {
      "cookie": {
        "sameSite": "lax"
      }
    }
  },
  "database": {
    "database": "./db.sqlite3",
    "synchronize": false
  }
}
